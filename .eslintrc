{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "plugin:jest/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "jsx-a11y",
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0
  },
  "globals": {
    "React": "writable"
  }
}